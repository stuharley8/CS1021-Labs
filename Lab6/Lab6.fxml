<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="harleys.Lab6Controller">
   <children>
      <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="58.0" text="URL" />
      <TextField fx:id="urlTextField" onAction="#analyze" prefHeight="25.0" prefWidth="323.0" text="http://msoe.us/taylor/cs1021/Lab6" />
      <Button mnemonicParsing="false" onAction="#analyze" prefHeight="25.0" prefWidth="119.0" text="Analyze" />
      <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="59.0" text="Size" />
      <TextField fx:id="sizeTextField" editable="false" prefHeight="25.0" prefWidth="90.0" />
      <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="136.0" text="Download Time" />
      <TextField fx:id="downloadTimeTextField" editable="false" prefHeight="25.0" prefWidth="215.0" />
      <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="60.0" text="Port" />
      <TextField fx:id="portTextField" editable="false" prefHeight="25.0" prefWidth="89.0" />
      <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="136.0" text="Host" />
      <TextField fx:id="hostTextField" editable="false" prefHeight="25.0" prefWidth="215.0" />
      <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="61.0" text="Timeout" />
      <TextField fx:id="timeoutTextField" onAction="#setTimeout" prefHeight="25.0" prefWidth="320.0" text="10000" />
      <Button mnemonicParsing="false" onAction="#setTimeout" prefHeight="25.0" prefWidth="119.0" text="Set" />
      <TextArea fx:id="webPageTextArea" prefHeight="301.0" prefWidth="501.0" wrapText="true" />
   </children>
</FlowPane>
